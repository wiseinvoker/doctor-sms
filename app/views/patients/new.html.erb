<!-- app/views/patients/new.html.erb -->
<h1>Sign Up</h1>

<%= form_with(model: @patient, url: patients_path) do |form| %>
  <%= form.label :full_name %>
  <%= form.text_field :full_name %>

  <%= form.label :phone_number %>
  <%= form.text_field :phone_number %>

  <%= form.submit "Sign Up" %>
<% end %>
